<header class="site site-header" component="header">
	<h1>{{=it.site_name}}</h1>
	<a href="/" class="site site-logo">
		{{=it.site_name}}
	</a>
	<nav class="site-header-nav">
		<h2>{{=it.site_description}}</h2>
		<ul>
			{{~it.site_nav:item:index}}
			<li>
				<a href="{{=item.nav_item_url}}">{{=item.nav_item_name}}</a>
			</li>
			{{~}}
			<li class="site clear"></li>
		</ul>
		<div class="site clear"></div>
	</nav>
	<div class="site clear"></div>
</header>
